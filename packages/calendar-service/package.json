{
  "name": "calendar-service",
  "version": "0.1.2",
  "description": "Allows you to search the calendar by day/week or even make specific searches in the calendar (particular courses, teachers etc...).",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "nodemon .",
    "lint": "eslint **/*.js --ignore-pattern internals/scripts",
    "lint:fix": "eslint **/*.js --ignore-pattern internals/scripts --fix",
    "prettier": "prettier --loglevel warn --write \"**/*.{t,j}s\"",
    "test": "cross-env NODE_ENV=test jest --force-exit --detectOpenHandles",
    "test:dev": "jest --detectOpenHandles --watchAll",
    "pack": "rm -rf pkg/*.tgz && npm pack && mkdir -p pkg && mv *.tgz pkg/",
    "build": "npm run update-ecosystem && npm run pack && npm run docker-build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "standard": "^16.0.3",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.6",
    "google-auth-library": "^6.1.6",
    "googleapis": "^67.0.0",
    "helmet": "^4.4.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "tmp": "^0.2.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  }
}
